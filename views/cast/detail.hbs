<div class="row">
    <div class="col-4">
        <img src={{cast.image}} class="w-100">
    </div>
    <div class="col-8">
        <div class="fw-bold text-uppercase fs-1">{{cast.name}}</div>
        <div> <span class="fw-bold">Realname:</span> {{cast.realName}}</div>
         <div> <span class="fw-bold">Gender:</span> {{cast.gender}}</div>
         <div> <span class="fw-bold">Heigh: </span>{{cast.heightCentimeters}} cm</div>
        <div> <span class="fw-bold">BirthDate:</span> {{cast.birthDate}}</div>
        <div> <span class="fw-bold">BirthPlace: </span>{{cast.birthPlace}}</div>
         <div> <span class="fw-bold">Nicknames: </span>
         {{#each nicknames}}
            {{Nickname}}/
         {{/each}}
         </div>

       
       
    </div>
    <div class="fw-bold fs-1">Movies</div>
    <table class="table p-2 m-2" >
        <thead>
            <tr>
                <th scope="col">Name</th>
                <th scope="col">Year</th>
            </tr>
        </thead>
        <tbody>
            {{#each movies}}
            <tr>
                <td><a href="/movie/{{id}}">{{title}}</a></td>
                <td>{{year}}</td>
            </tr>
            {{/each}}
        </tbody>
    </table>
</div>